


#
# git
alias gs='git status'
alias gcam='git commit -a -m'
alias gcm='git commit -m'
alias gp='git push'
alias ga='git add'
alias ga.='git add .'
alias grm='git rm'
alias gl='git log --oneline --graph'

alias cdp='cap deploy'
alias gpd='git push;cap deploy'
alias gdm='git diff | mate'
alias gd='git diff'

#
# nav

alias ls='ls -apG'
alias lsl='ls -ahlpG'
alias cl='clear'
alias cd..='cd ..'
alias .='lsl'
alias ..='cd ..'
alias l='lsl'
alias ..l='cd ..;lsl'
alias ll='ls -ahlpG *'

#
# servers
alias restart='touch tmp/restart.txt'


#
# shortcuts

alias rbv='rbenv'

alias tarc='tar -czpvf'
alias tarx='tar -xzpvf'
